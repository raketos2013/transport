                <col style="min-width:180px;width:180px;">
                <col style="min-width:110px;width:100%;">
            </colgroup>
            <thead>
                <tr>
                    <th>
                        <p></p>
                        <div class="separate"></div>
                        <div class="sorter"></div>
                    </th>
                    <th onclick="sortTable(1)">
