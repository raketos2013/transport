        <table class="table-header">
            <colgroup>
                <col style="min-width:140px;width:140px;">
                <col style="min-width:135px;width:135px;">
                <col style="min-width:140px;width:140px;">
                <col style="min-width:140px;width:140px;">
                <col style="min-width:150px;width:150px;">
                <col style="min-width:140px;width:140px;">
