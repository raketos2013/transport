                        <div class="separate"></div>
                        <div class="sorter"></div>
                    </th>
                    <th>
                        <p>Статус</p>
                        <div class="separate"></div>
                        <div class="sorter"></div>
                    </th>
                </tr>
            </thead>
        </table>
