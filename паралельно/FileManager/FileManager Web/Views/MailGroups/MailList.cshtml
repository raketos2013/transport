@model IEnumerable<FileManager.Domain.Entity.MailList>
@{

}


<div class="container-fluid">
	<table>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@item.EMail
				</td>
				<td>
					<button class="btn p-0" data-bs-toggle="modal" data-bs-target="#staticBackdropDeleteEmail" onclick="delEmail('@item.EMail')">
						<img src="~/images/delete_icon.png" />
					</button>
				</td>
			</tr>
		}
	</table>


</div>